	transform: translateY(0px);
}
.animation-element.bio {
	position relative
	opacity 1
	transition all 1s ease
	transform translateY(0)
}
.top-of-next {
	width: 100%;
	height: 41px;
	background: #7DDCEB;
	position: absolute;
	top: 0;

}
.top-of-next::before {
	content: '';
	position: absolute;
	width: 0;
	height: 0;
	border-style: solid;
	border-color: transparent;
	border-bottom: 0;
	top: 0;
	left: 50%;
	border-top-color: #57D2E5;
	border-width: 16px;
	transform: translateX(-50%);
	z-index: 4;
}
.show-rest {
	width: 100%;
	height: 41px;
	background: #E9E092;
	position: absolute;
	bottom: 0;

}
.show-rest::after {
	content: '';
	position: absolute;
	width: 0;
	height: 0;
	border-style: solid;
	border-color: transparent;
	border-bottom: 0;
	top: 0;
	left: 50%;
	border-top-color: #E1D56B;
	border-width: 16px;
	transform: translateX(-50%);
	z-index: 4;
